source $TA_SOURCE_CONF/zshrc.sh
RESET="$(tput sgr0)"
RED=1
MUSTARD=136
DARKGRAY=237

FRED="$(tput setaf $RED)"
FMUSTARD="$(tput setaf $MUSTARD)"
BDARKGRAY="$(tput setab $DARKGRAY)"

PROMPT_LEFT=$FRED
PROMPT_LEFT+=%n
PROMPT_LEFT+=$RESET' on '
PROMPT_LEFT+=$FMUSTARD%M
PROMPT_LEFT+=$RESET
PROMPT_LEFT+=$'\n'

PROMPT_RIGHT=$BDARKGRAY$FRED%~$RESET
PROMPT_RIGHT+='$(git_super_status)'
PROMPT_RIGHT+=$'\n'
PROMPT_RIGHT+='âžœ '

PROMPT=$PROMPT_LEFT$PROMPT_RIGHT
